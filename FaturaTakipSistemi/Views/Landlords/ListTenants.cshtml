@{
    ViewData["Title"] = "Tenants";
}

<h2>@ViewData["Title"]</h2>

<table class="table">
    <thead>
        <tr>
            <th>Goverment ID</th>
            <th>Name</th>
            <th>Last Name</th>
            <th>Phone</th>
            <th>Email</th>
            <th>Manage</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.GovermentId</td>
                <td>@user.Name</td>
                <td>@user.LastName</td>
                <td>@user.Phone</td>
                <td>@user.Email</td>
                <td>
                    <a class="btn btn-primary" href="tenants/@user.Id">Edit</a>
                </td>
            </tr>
        }
    </tbody>
</table>